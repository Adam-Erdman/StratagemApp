        android:supportsRtl="true"
        android:theme="@style/Theme.StratagemApp">
        <activity android:name=".detailView"></activity>
        <activity android:name=".stratagemViewer" />
        <activity android:name=".unitSelectionPage" />
        <activity android:name=".phasePage" />
